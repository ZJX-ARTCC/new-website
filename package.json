		"@sentry/sveltekit": "^8.47.0",
		"@sveltejs/adapter-node": "^5.2.11",
		"autoprefixer": "^10.4.20",
		"colorthief": "^2.6.0",
		"dotenv": "^16.4.7",
		"flowbite-svelte": "^0.47.4",
		"flowbite-svelte-icons": "^0.4.5",
		"globals": "^15.14.0",
		"postcss": "^8.4.49",
		"prisma": "^6.1.0",
		"svelte-use-form": "^2.10.0",
		"tailwindcss": "^3.4.17"
	},
	"volta": {
		"node": "21.5.0"
	}
}
